<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC   
    "-//mybatis.org//DTD Mapper 3.0//EN"  
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.MerchandiseDao">

	<select id="search" resultType="entity.Merchandise">
		select * from merchandise
	</select>

	<select id="searchById" resultType="entity.Merchandise">
		select * from merchandise
		where id=#{0}
	</select>

</mapper> 
    